package tues_17_jan;

public class String7 {
	public static void main(String args[])
	{
		String a="ADAI";
		String res=sp(a);
		System.out.println(res);
		boolean flag=a.equals(res);
		
		if(flag==true)
		{
			System.out.println("Palingdrom");
		}
		else
		{
			System.out.println("not");
		}
		/*if(a.equals(res))
		{
		System.out.println("string is Palingdrom"+res);
		}
		else
		{
			System.out.println("String is not Palingdrom!");
		}*/
		
	}
	public static String sp(String a)
	{
		String b="";
		for(int i=0; i<a.length(); i++)
		{
			char ch=a.charAt(i);
			b=ch+b;
		}
		return (b);
	}

}
