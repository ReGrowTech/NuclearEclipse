package sundayfunday;

public class Pro1 
{
	public static void main(String args[])
	{
		String s="Regrow Tech India";
		String a[]=s.split(" ");
		for(int i=0; i<a.length;i++)
		{
			String str="";
			for(int j=0;j<a.length;j++)
			{
				str=a[0]+a[a.length-1];
			}
			System.out.println(str);
		}
		
	}

}
