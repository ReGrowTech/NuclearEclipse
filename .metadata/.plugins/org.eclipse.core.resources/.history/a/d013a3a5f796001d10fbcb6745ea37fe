package wed_18_jan;

public class SortingArray {

	public static void main(String[] args) {

		int a[]= {10,5,2,4,6,2,8};
		int temp=0;
		for(int i=0; i<a.length; i++)
		{
			for(int j=i+1; j<a.length; j++)
			{
				if(a[i]>a[j])
				{
					temp=a[i];
					a[i]=a[j];
					a[j]=temp;
				}
			}
		}
		for(int each:a)
		{
			System.out.println(each);
		}
		
	}
}
