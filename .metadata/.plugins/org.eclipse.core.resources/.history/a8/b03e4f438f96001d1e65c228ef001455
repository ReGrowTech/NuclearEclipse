package tues_17_jan;

public class String7 {
	public static void main(String args[])
	{
		String a="Adil";
		String res=sp(a);
		if(a==res)
		{
		System.out.println("string is Palingdrom"+res);
		}
		else
		{
			System.out.println("String is not Palingdrom!");
		}
		
	}
	public static String sp(String a)
	{
		String b="";
		for(int i=0; i<a.length(); i++)
		{
			char ch=a.charAt(i);
			b=ch+b;
		}
		return (b);
	}

}
